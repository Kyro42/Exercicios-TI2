import java.util.*;

class somar {
	public static Scanner scan = new Scanner(System.in);
	
	public static void main(String[] args) {
		int num1, num2, soma;
		
		System.out.println("Digite o primeiro numero: ");
		num1 = scan.nextInt();
		System.out.println("Digite o segundo numero: ");
		num2 = scan.nextInt();
		
		soma=num1+num2;
		
		System.out.printf("%d", soma);
	}

}
